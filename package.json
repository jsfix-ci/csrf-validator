    "express",
    "expressjs",
    "nest",
    "nestjs",
    "npm",
    "node",
    "nodejs",
    "typescript",
    "library"
  ],
  "author": "Sajeer Babu <sajeerzeji44@gmail.com> (https://www.linkedin.com/in/sajeerzeji/)",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "express": "^4.17.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "http-errors": "^1.8.0",
    "http-status": "^1.5.0",
    "bcrypt": "^5.0.1",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/supertest": "^2.0.11",
