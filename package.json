{
  "name": "csrf-validator",
  "version": "1.0.5",
  "description": "A CSRF Validator for nodejs apps",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "npm install && npm run build && mocha --check-leaks --reporter spec --bail test/",
